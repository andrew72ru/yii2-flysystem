{
  "name": "andrew72ru/yii2-flysystem",
  "description": "The flysystem extension for the Yii framework",
  "keywords": [
    "yii2",
    "flysystem",
    "filesystem",
    "files",
    "aws",
    "s3",
    "azure",
    "copy.com",
    "dropbox",
    "ftp",
    "gridfs",
    "rackspace",
    "sftp",
    "webdav"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexander Kochetov",
      "email": "creocoder@gmail.com"
    },
    {
      "name": "Andrew Zhdanovskih",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "league/flysystem": "~1.0",
    "andrew72ru/flysystem-gridfs": "~1.0.1",
    "mongodb/mongodb": "~1.1",
    "yiisoft/yii2-mongodb": "^2.1"
  },
  "require-dev": {
    "league/flysystem-aws-s3-v2": "~1.0",
    "league/flysystem-azure": "~1.0",
    "league/flysystem-cached-adapter": "~1.0",
    "league/flysystem-copy": "~1.0",
    "league/flysystem-dropbox": "~1.0",
    "league/flysystem-rackspace": "~1.0",
    "league/flysystem-replicate-adapter": "~1.0",
    "league/flysystem-sftp": "~1.0",
    "league/flysystem-webdav": "~1.0",
    "league/flysystem-ziparchive": "~1.0",
    "codeception/codeception": "~2.2",
    "codeception/verify": "~0.3.1"
  },
  "repositories": [
    {
      "type": "pear",
      "url": "https://pear.php.net"
    },
    {
      "type": "vcs",
      "url": "https://github.com/andrew72ru/flysystem-gridfs.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "andrew72ru\\flysystem\\": "src"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-pattern-skip-version": "(-build)"
  }
}
